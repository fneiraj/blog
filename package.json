{
  "name": "astro-paper",
  "version": "4.2.0",
  "private": false,
  "scripts": {
    "dev": "wrangler pages dev --proxy 3000 --kv PAGE_COUNTER PAGE_VIEW_RECORDS -- astro dev",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^3.1.3",
    "@astrojs/preact": "^3.5.1",
    "@astrojs/rss": "^4.0.7",
    "@docsearch/css": "^3.6.1",
    "@docsearch/js": "3.6.0",
    "@expressive-code/core": "^0.35.6",
    "@expressive-code/plugin-collapsible-sections": "^0.35.6",
    "@expressive-code/plugin-line-numbers": "^0.35.6",
    "@resvg/resvg-js": "^2.6.2",
    "astro": "^4.14.2",
    "astro-expressive-code": "^0.35.6",
    "astro-icon": "^1.1.1",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "mdast-util-to-string": "^4.0.0",
    "preact": "^10.23.2",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "satori": "^0.10.14",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.1.6",
    "@astrojs/tailwind": "^5.1.0",
    "@cloudflare/workers-types": "^4.20240815.0",
    "@divriots/jampack": "^0.23.4",
    "@tailwindcss/typography": "^0.5.12",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "algoliasearch": "^4.23.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-astro": "0.34.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.3.1",
    "eslint-plugin-promise": "6.1.1",
    "gray-matter": "^4.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "remove-markdown": "^0.5.0",
    "wrangler": "^3.72.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "prettier --check . --plugin=prettier-plugin-astro && git add -A ."
    }
  }
}
